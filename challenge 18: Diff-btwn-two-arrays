function diffArray(arr1, arr2) {
	var newArr = [];

	console.log(arr1, arr2);

	var nums2 = [];
	for(var i=0; i<arr2.length; i++){
	
	if((arr1.indexOf(arr2[i]) === -1)){
		nums2.push(arr2[i])}

		}

	for(var j=0; j<arr1.length; j++){

		if(arr2.indexOf(arr1[j]) === -1) {
			num2.push(arr1[j])
		}

		}
	return nums2;


}

diffArray([1, 2, 3, 5], [1, 2, 3, 4, 5]);
